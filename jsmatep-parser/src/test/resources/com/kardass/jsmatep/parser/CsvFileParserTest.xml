    <ns0:type>
        <ns0:csv delimiter=";" field-count="7"/>
    </ns0:type>
    <ns0:value-object>com.kardass.jsmatep.parser.ImporterTestValueObject</ns0:value-object>
    <ns0:skip-record>
        <ns0:starts-with>#</ns0:starts-with>
    </ns0:skip-record>
    <ns0:fields>
        <ns0:field name="a" bean-property-name="someOtherName" position="0" type="String"></ns0:field>
        <ns0:field name="b" position="1" type="Date" format="yyyyMMdd"></ns0:field>
        <ns0:field name="c" position="2" type="Integer"></ns0:field>
        <ns0:field name="d" position="3" type="float"></ns0:field>
        <ns0:field name="e" position="4" type="Decimal"></ns0:field>
        <ns0:field name="f" position="5" type="String"></ns0:field>
        <ns0:field name="g" position="6" type="Decimal" format="3,2"></ns0:field>
<!--
        <ns0:field name="f" position="5" max-length="1" type="String"></ns0:field>
        <ns0:field name="g" position="6" min-length="2" max-length="6" type="Decimal" format="3,2"></ns0:field>
-->
    </ns0:fields>
</ns0:jSmaTeP-Parser-Config>
